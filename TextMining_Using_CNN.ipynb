      "source": [
        "#Remove punctuation using regaX\n",
        "import re\n",
        "import string  \n",
        "def remove_punct(content):\n",
        "    content_nopunct = ''\n",
        "    content_nopunct = re.sub('['+string.punctuation+']', '', content)\n",
        "    return content_nopunct\n",
        "data1['Content_Clean'] = data1['content'].apply(lambda x: remove_punct(x))"
      ],
      "execution_count": 0,
      "outputs": []
